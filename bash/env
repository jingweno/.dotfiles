export CLOJURE_CONTRIB_HOME="/usr/local/Cellar/clojure-contrib/1.2.0/clojure-contrib.jar"
export CLASSPATH=$CLASSPATH:$CLOJURE_CONTRIB

export JRUBY_OPTS=--1.9

